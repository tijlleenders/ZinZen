{
  "name": "zinzen-frontend",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/components/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/components/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@fontsource/kristi": "^4.5.2",
    "@fontsource/montserrat": "^4.5.3",
    "@jest/globals": "^28.1.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "bootstrap": "^5.1.3",
    "dexie": "^3.2.2",
    "framer-motion": "^6.2.8",
    "history": "^5.3.0",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "lokijs": "^1.5.12",
    "moment": "^2.29.2",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-bootstrap-icons": "^1.8.2",
    "react-dark-mode-toggle": "0.2.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-router": "^6.0.0-beta.8",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "recoil": "^0.7.1",
    "sass": "^1.50.0",
    "vite": "^2.9.1",
    "web-vitals": "^0.2.4",
    "workbox": "^0.0.0",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4",
    "yui-lint": "^0.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "CI=false && vite build",
    "preview": "vite preview",
    "test": "jest --verbose ./src/__tests__",
    "cypresstest": "cypress run",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write src/components/**/*.{html,css,less,ejs}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cypress": "9.4.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "vite-plugin-pwa": "^0.12.0",
    "workbox-cli": "^6.5.3"
  }
}
